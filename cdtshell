
# generate diffs
./cdtshell.sh -Source XML -SourcePassword -Target local -TargetPassword Sterling12 -DoNotSynchronize Y -ExportDir /media/psf/Home/Downloads/documents/lowes/CDT/cdt_swd/swd_diffs


# apply diffs
